<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.zldzs.dao.ITestConnDao">

	<insert id="add" parameterType="java.util.Map">
		insert into z_demo(id,name,date,flag) values(2,#{name},#{date},#{flag})
	</insert>
	
	<insert id="add1" parameterType="java.util.Map">
		insert into z_demo2(id,name) values(2,#{name})
	</insert>
	
	
</mapper>